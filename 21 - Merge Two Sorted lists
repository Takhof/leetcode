var mergeTwoLists = function(l1, l2) {
    let dummy = {val:0, next:null},
        current = dummy;
    
    while(l1 && l2) {
        if(l1.val > l2.val) {
            current.next = l2;
            l2 = l2.next;
        } else {
            current.next = l1;
            l1 = l1.next;
        }
        current = current.next;
    }
    current.next = l1 || l2;
    
    return dummy.next;
};